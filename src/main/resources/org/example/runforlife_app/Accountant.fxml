<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="600.0" minWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.runforlife_app.Administratie.AdministratieRunnerController">
   <center>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="runnerTableView">
              <columns>
                <TableColumn fx:id="nameColumn" minWidth="120.0" prefWidth="-1.0" text="Name" />
                <TableColumn fx:id="addressColumn" minWidth="120.0" prefWidth="-1.0" text="Address" />
                  <TableColumn fx:id="postalCodeColumn" minWidth="120.0" prefWidth="-1.0" text="Postal Code" />
                  <TableColumn fx:id="cityColumn" minWidth="120.0" prefWidth="-1.0" text="City" />
                  <TableColumn fx:id="phoneNumberColumn" minWidth="120.0" prefWidth="-1.0" text="Phone Number" />
                  <TableColumn fx:id="registrationFeeColumn" minWidth="120.0" prefWidth="-1.0" text="Registration Fee" />
              </columns>
            </TableView>
            <VBox alignment="CENTER" minWidth="80.0">
               <children>
                  <Button mnemonicParsing="false" text="Toevoegen">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" text="Aanpassen">
                     <VBox.margin>
                        <Insets top="50.0" />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" text="Verwijderen">
                     <VBox.margin>
                        <Insets top="50.0" />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" text="Import">
                     <VBox.margin>
                        <Insets top="50.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </center>
   <top>
      <BorderPane style="-fx-background-color: #032139;" BorderPane.alignment="CENTER">
         <left>
            <Button minHeight="50.0" minWidth="80.0" mnemonicParsing="false" text="Button" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets right="129.0" />
               </BorderPane.margin>
            </Button>
         </left>
         <center>
            <Label text="Administratie" textAlignment="CENTER" textFill="WHITE" BorderPane.alignment="CENTER">
               <font>
                  <Font size="40.0" />
               </font>
            </Label>
         </center>
         <right>
            <ImageView fitHeight="199.0" fitWidth="209.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <image>
                  <Image url="@../../../RunForLife%20Logo.png" />
               </image>
            </ImageView>
         </right>
      </BorderPane>
   </top>
</BorderPane>
